{
  "name": "neural-trader",
  "version": "2.3.2",
  "description": "High-performance neural trading system with GPU acceleration, real-time execution, multi-agent swarm coordination, and 16+ production-ready examples spanning finance, healthcare, energy, and logistics",
  "main": "index.js",
  "workspaces": [
    "packages/core",
    "packages/predictor",
    "packages/examples/*",
    "packages/examples/shared/*"
  ],
  "bin": {
    "neural-trader": "./bin/cli.js"
  },
  "scripts": {
    "install": "node scripts/install.js",
    "postinstall": "node scripts/postinstall.js",
    "check-binaries": "node scripts/check-binaries.js",
    "build": "cd neural-trader-rust/crates/napi-bindings && napi build --platform --release",
    "build:debug": "cd neural-trader-rust/crates/napi-bindings && napi build --platform",
    "build:release": "cd neural-trader-rust/crates/napi-bindings && napi build --platform --release --strip",
    "build:all": "cd neural-trader-rust/crates/napi-bindings && napi build --platform --release --strip --target x86_64-pc-windows-msvc --target x86_64-apple-darwin --target aarch64-apple-darwin --target x86_64-unknown-linux-gnu --target aarch64-unknown-linux-gnu",
    "build:cargo": "cd neural-trader-rust && cargo build --release",
    "artifacts": "cd neural-trader-rust/crates/napi-bindings && napi artifacts",
    "prepublishOnly": "npm run build:release && npm run artifacts",
    "test": "cd neural-trader-rust && cargo test --workspace",
    "test:napi": "node neural-trader-rust/crates/napi-bindings/test/integration.test.js",
    "test:e2b": "jest tests/e2b --testTimeout=60000",
    "bench": "cd neural-trader-rust && cargo bench",
    "bench:swarm": "jest tests/e2b/swarm-benchmarks.test.js --testTimeout=600000",
    "bench:swarm:fast": "jest tests/e2b/swarm-benchmarks.test.js --testTimeout=600000 -t 'Creation Performance'",
    "bench:swarm:full": "jest tests/e2b/swarm-benchmarks.test.js --testTimeout=600000 --verbose",
    "bench:swarm:report": "jest tests/e2b/swarm-benchmarks.test.js --testTimeout=600000 && cat docs/e2b/SWARM_BENCHMARKS_REPORT.md",
    "benchmark": "node --expose-gc tests/benchmarks/run-all.js",
    "benchmark:all": "node --expose-gc tests/benchmarks/run-all.js --export-json --export-html",
    "benchmark:functions": "node tests/benchmarks/function-performance.benchmark.js",
    "benchmark:scalability": "node --expose-gc tests/benchmarks/scalability.benchmark.js",
    "benchmark:gpu": "node tests/benchmarks/gpu-comparison.benchmark.js",
    "benchmark:quick": "node tests/benchmarks/run-all.js --quick",
    "benchmark:report": "node tests/benchmarks/run-all.js --export-html && open tests/benchmarks/results/*.html",
    "version": "cd neural-trader-rust/crates/napi-bindings && napi version"
  },
  "keywords": [
    "trading",
    "neural-networks",
    "algorithmic-trading",
    "finance",
    "mcp",
    "rust",
    "high-performance",
    "gpu-acceleration",
    "backtesting",
    "portfolio-optimization",
    "healthcare",
    "logistics",
    "energy-grid",
    "supply-chain",
    "quantum-optimization",
    "anomaly-detection",
    "adaptive-systems",
    "market-microstructure",
    "swarm-intelligence",
    "self-learning"
  ],
  "author": "Neural Trader Team",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/neural-trader.git"
  },
  "bugs": {
    "url": "https://github.com/ruvnet/neural-trader/issues"
  },
  "homepage": "https://github.com/ruvnet/neural-trader#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "napi": {
    "name": "neural-trader",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "dependencies": {
    "agentdb": "^1.6.1",
    "agentic-flow": "^1.10.2",
    "agentic-payments": "^0.1.13",
    "aidefence": "^2.1.1",
    "e2b": "^2.6.4",
    "ioredis": "^5.8.2",
    "midstreamer": "^0.2.4",
    "sublinear-time-solver": "^1.5.0"
  },
  "devDependencies": {
    "@e2b/code-interpreter": "^2.2.0",
    "@napi-rs/cli": "^3.4.1",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^20.19.25",
    "benchmark": "^2.1.4",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "jest": "^29.7.0",
    "microtime": "^3.1.1",
    "ora": "^9.0.0"
  },
  "optionalDependencies": {
    "@neural-trader/core": "^1.0.1",
    "@neural-trader/predictor": "^0.1.0",
    "neural-trader-darwin-arm64": "2.2.0",
    "neural-trader-darwin-x64": "2.2.0",
    "neural-trader-linux-arm64": "2.2.0",
    "neural-trader-linux-x64": "2.2.0",
    "neural-trader-win32-x64": "2.2.0",
    "@neural-trader/example-adaptive-systems": "^1.0.0",
    "@neural-trader/example-anomaly-detection": "^1.0.0",
    "@neural-trader/example-dynamic-pricing": "^1.0.0",
    "@neural-trader/example-energy-forecasting": "^1.0.0",
    "@neural-trader/example-energy-grid-optimization": "^1.0.0",
    "@neural-trader/example-evolutionary-game-theory": "^1.0.0",
    "@neural-trader/example-healthcare-optimization": "^1.0.0",
    "@neural-trader/example-logistics-optimization": "^1.0.0",
    "@neural-trader/example-market-microstructure": "^1.0.0",
    "@neural-trader/example-multi-strategy-backtest": "^1.0.0",
    "@neural-trader/example-neuromorphic-computing": "^1.0.0",
    "@neural-trader/example-portfolio-optimization": "^1.0.0",
    "@neural-trader/example-quantum-optimization": "^1.0.0",
    "@neural-trader/example-supply-chain-prediction": "^1.0.0",
    "@neural-trader/example-benchmarks": "^1.0.0",
    "@neural-trader/example-test-framework": "^1.0.0"
  },
  "files": [
    "bin/",
    "index.js",
    "README.md",
    "LICENSE",
    "scripts/",
    "neural-trader-rust/crates/napi-bindings/*.node",
    "neural-trader-rust/crates/napi-bindings/index.js",
    "neural-trader-rust/crates/napi-bindings/index.d.ts",
    "packages/core/src/",
    "packages/core/package.json",
    "packages/core/README.md",
    "packages/core/tsconfig.json",
    "packages/predictor/dist/",
    "packages/predictor/package.json",
    "packages/predictor/README.md",
    "packages/predictor/examples/"
  ]
}
