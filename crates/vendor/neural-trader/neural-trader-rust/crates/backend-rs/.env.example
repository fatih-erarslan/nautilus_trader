# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=3001

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/beclever
DATABASE_POOL_SIZE=10

# Redis
REDIS_URL=redis://localhost:6379

# Authentication
JWT_SECRET=your-jwt-secret-here-change-in-production
JWT_EXPIRY=7d

# CORS
CORS_ORIGIN=http://localhost:5173
CORS_CREDENTIALS=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# E2B
E2B_API_KEY=your-e2b-api-key

# Anthropic
ANTHROPIC_API_KEY=your-anthropic-api-key

# FoxRuv Packages
AGENTIC_FLOW_MODE=production
AGENTIC_FLOW_MAX_AGENTS=66
AGENTIC_FLOW_ENABLE_BOOSTER=true
AGENTIC_FLOW_ENABLE_REASONING_BANK=true

AGENTDB_MODE=memory
AGENTDB_INDEX=hnsw
AGENTDB_DIMENSIONS=1536
AGENTDB_ENABLE_RL=true

MIDSTREAMER_ENABLE_WASM=true
MIDSTREAMER_STREAMING=true

AIDEFENCE_ENABLE_REALTIME=true
AIDEFENCE_DETECTION_THRESHOLD=0.95

# Logging
LOG_LEVEL=info
DEBUG=false
