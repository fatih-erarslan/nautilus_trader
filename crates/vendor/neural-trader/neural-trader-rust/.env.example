# Neural Trader Environment Variables Template
# Copy this file to .env and fill in your values
# WARNING: Never commit .env to version control!

# Environment
ENVIRONMENT=development
RUST_LOG=info
RUST_BACKTRACE=1

# Database
DATABASE_URL=postgresql://neural:trader@localhost:5432/neural_trader_dev
POSTGRES_PASSWORD=trader_dev_password

# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=redis_dev_password

# Alpaca (Paper Trading)
ALPACA_API_KEY=PKXXXXXXXXXXXXXX
ALPACA_API_SECRET=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Binance (Optional)
BINANCE_API_KEY=
BINANCE_API_SECRET=

# AgentDB
AGENTDB_PATH=./data/agentdb

# TLS (Production only)
TLS_CERT_PATH=/etc/neural-trader/tls/cert.pem
TLS_KEY_PATH=/etc/neural-trader/tls/key.pem

# Monitoring
JAEGER_ENDPOINT=http://localhost:14268/api/traces
GRAFANA_PASSWORD=admin

# Application
NEURAL_TRADER_CONFIG=.config/development.toml
NEURAL_TRADER_LOG_LEVEL=info
NEURAL_TRADER_PORT=8080
