{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "/tools/get_execution_analytics.json",
  "title": "get_execution_analytics",
  "description": "Get trade execution analytics.",
  "category": "analytics",
  "type": "object",
  "properties": {
    "input_schema": {
      "type": "object",
      "properties": {
        "time_period": {
          "type": "string",
          "default": "1h",
          "enum": [
            "1h",
            "24h",
            "7d",
            "30d"
          ]
        }
      }
    },
    "output_schema": {
      "type": "object",
      "properties": {
        "total_executions": {
          "type": "integer"
        },
        "average_latency": {
          "type": "number"
        },
        "success_rate": {
          "type": "number"
        },
        "slippage": {
          "type": "object",
          "properties": {
            "average": {
              "type": "number"
            },
            "max": {
              "type": "number"
            }
          }
        }
      }
    }
  },
  "metadata": {
    "cost": "low",
    "latency": "fast",
    "gpu_capable": false,
    "version": "2.0.0"
  }
}
