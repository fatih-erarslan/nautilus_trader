{
  "name": "@neural-trader/example-evolutionary-game-theory",
  "version": "1.0.0",
  "description": "Neural AI evolutionary game theory with self-learning multi-agent tournaments, replicator dynamics, ESS calculation, genetic algorithms, and strategic adaptation for competitive systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "example:basic": "node dist/examples/basic-game.js",
    "example:tournament": "node dist/examples/tournament-evolution.js",
    "example:swarm": "node dist/examples/swarm-learning.js",
    "lint": "eslint src tests examples --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "evolutionary-game-theory",
    "replicator-dynamics",
    "ess",
    "prisoners-dilemma",
    "hawk-dove",
    "game-theory",
    "multi-agent",
    "tournament",
    "self-learning",
    "genetic-algorithm",
    "agentdb",
    "agentic-flow",
    "neural-trader",
    "ai",
    "neural-network",
    "machine-learning",
    "optimization",
    "typescript",
    "swarm-intelligence",
    "nash-equilibrium",
    "strategy-evolution",
    "cooperation",
    "competition"
  ],
  "author": "Neural Trader Team",
  "license": "MIT",
  "dependencies": {
    "@neural-trader/predictor": "workspace:*",
    "@neural-trader/core": "workspace:*",
    "agentdb": "^1.6.1",
    "agentic-flow": "^1.10.2",
    "openai": "^4.0.0",
    "mathjs": "^12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
