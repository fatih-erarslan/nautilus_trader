    "type": "git",
    "url": "https://github.com/ruvnet/neural-trader.git",
    "directory": "packages/examples/evolutionary-game-theory"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
