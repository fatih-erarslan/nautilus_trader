{
  "name": "@neural-trader/example-quantum-optimization",
  "version": "1.0.0",
  "description": "Neural AI quantum-inspired optimization with QAOA, VQE, quantum annealing, swarm-based circuit exploration, and variational algorithms for combinatorial optimization and constraint problems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "example:qaoa": "node dist/examples/qaoa-maxcut.js",
    "example:vqe": "node dist/examples/vqe-optimization.js",
    "example:annealing": "node dist/examples/quantum-annealing.js",
    "example:swarm": "node dist/examples/swarm-circuits.js",
    "lint": "eslint src tests --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "quantum-optimization",
    "qaoa",
    "vqe",
    "quantum-annealing",
    "variational-algorithms",
    "combinatorial-optimization",
    "swarm-circuits",
    "agentdb",
    "sublinear-time",
    "neural-trader",
    "ai",
    "neural-network",
    "machine-learning",
    "self-learning",
    "optimization",
    "typescript",
    "agentic-flow",
    "quantum-computing",
    "quantum-inspired",
    "maxcut",
    "graph-optimization",
    "constraint-satisfaction",
    "variational-quantum"
  ],
  "author": "Neural Trader Team",
  "license": "MIT",
  "dependencies": {
    "@neural-trader/core": "workspace:*",
    "@neural-trader/predictor": "workspace:*",
    "agentdb": "^1.0.0",
    "agentic-flow": "^1.0.0",
    "openai": "^4.0.0",
    "mathjs": "^12.0.0",
    "numeric": "^1.2.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/numeric": "^1.2.2",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
