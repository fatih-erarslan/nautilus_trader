{
  "name": "@neural-trader/example-healthcare-optimization",
  "version": "1.0.0",
  "description": "Neural AI healthcare optimization with self-learning patient forecasting, intelligent staff scheduling, queue theory modeling, and swarm-based resource allocation for hospital management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "dev": "tsx watch src/index.ts",
    "example": "tsx examples/basic-usage.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "healthcare",
    "optimization",
    "machine-learning",
    "queue-theory",
    "scheduling",
    "forecasting",
    "neural-trader",
    "patient-flow",
    "hospital-management",
    "ai",
    "neural-network",
    "self-learning",
    "swarm-intelligence",
    "typescript",
    "agentdb",
    "agentic-flow",
    "resource-allocation",
    "capacity-planning",
    "real-time",
    "prediction",
    "operations-research",
    "healthcare-analytics",
    "medical-optimization"
  ],
  "author": "Neural Trader Team",
  "license": "MIT OR Apache-2.0",
  "homepage": "https://github.com/ruvnet/neural-trader#readme",
  "bugs": {
    "url": "https://github.com/ruvnet/neural-trader/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/neural-trader.git",
    "directory": "packages/examples/healthcare-optimization"
  },
  "dependencies": {
    "@neural-trader/predictor": "workspace:*",
    "agentdb": "^1.0.0",
    "agentic-flow": "^2.0.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [".ts"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
