{
  "name": "@neural-trader/example-adaptive-systems",
  "version": "1.0.0",
  "description": "Neural AI self-organizing multi-agent systems with swarm intelligence, boids, ant colony optimization, cellular automata, emergence detection, and adaptive behavior for complex systems modeling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "ts-node src/index.ts",
    "native:build": "cd native && cargo build --release",
    "native:test": "cd native && cargo test",
    "demo:boids": "ts-node examples/traffic-flow.ts",
    "demo:ants": "ts-node examples/crowd-dynamics.ts",
    "demo:automata": "ts-node examples/market-behavior.ts",
    "demo:ecosystem": "ts-node examples/ecosystem.ts",
    "lint": "eslint src tests examples --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "neural-trader",
    "swarm-intelligence",
    "boids",
    "ant-colony-optimization",
    "cellular-automata",
    "emergence",
    "self-organizing-systems",
    "multi-agent-systems",
    "adaptive-behavior",
    "agentdb",
    "agentic-flow",
    "machine-learning",
    "ai",
    "neural-network",
    "self-learning",
    "optimization",
    "typescript",
    "rust",
    "napi",
    "complex-systems",
    "collective-intelligence",
    "distributed-systems",
    "agent-based-modeling"
  ],
  "author": "Neural Trader Team",
  "license": "MIT",
  "dependencies": {
    "@neural-trader/predictor": "workspace:*",
    "@neural-trader/core": "workspace:*",
    "agentdb": "^2.0.0",
    "agentic-flow": "^1.0.0",
    "openai": "^4.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/neural-trader.git",
    "directory": "packages/examples/adaptive-systems"
  },
  "homepage": "https://github.com/ruvnet/neural-trader#readme",
  "bugs": {
    "url": "https://github.com/ruvnet/neural-trader/issues"
  },
  "license": "MIT OR Apache-2.0",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
