{
  "dashboard": {
    "id": null,
    "title": "Bayesian VaR Production Monitoring - Constitutional Prime Directive",
    "tags": ["bayesian-var", "production", "constitutional-prime-directive", "financial-systems"],
    "style": "dark",
    "timezone": "UTC",
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Constitutional Prime Directive Compliance",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "VIOLATION", "color": "red"},
                  "1": {"text": "COMPLIANT", "color": "green"}
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        },
        "targets": [
          {
            "expr": "min(bayesian_var_active_connections > 0, bayesian_model_accuracy_score >= 0.95, rate(bayesian_var_calculations_failed_total[5m]) <= 0.01)",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "System Availability (SLO: 99.99%)",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "max": 100,
            "min": 99.9,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 99.9},
                {"color": "yellow", "value": 99.95},
                {"color": "green", "value": 99.99}
              ]
            },
            "unit": "percent"
          }
        },
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "targets": [
          {
            "expr": "(1 - (rate(bayesian_var_calculations_failed_total[5m]) / rate(bayesian_var_total_calculations[5m]))) * 100",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "VaR Calculation Performance",
        "type": "graph",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 8},
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "logBase": 1,
            "max": null,
            "min": null,
            "show": true
          },
          {
            "label": "Rate",
            "logBase": 1,
            "max": null,
            "min": null,
            "show": true
          }
        ],
        "xAxis": {
          "buckets": null,
          "mode": "time",
          "name": null,
          "show": true,
          "values": []
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, bayesian_var_calculation_duration_seconds)",
            "refId": "A",
            "legendFormat": "P50 Latency"
          },
          {
            "expr": "histogram_quantile(0.95, bayesian_var_calculation_duration_seconds)",
            "refId": "B",
            "legendFormat": "P95 Latency"
          },
          {
            "expr": "histogram_quantile(0.99, bayesian_var_calculation_duration_seconds)",
            "refId": "C",
            "legendFormat": "P99 Latency"
          },
          {
            "expr": "rate(bayesian_var_calculations_successful_total[5m])",
            "refId": "D",
            "legendFormat": "Success Rate",
            "yAxis": 2
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rates & Constitutional Violations",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17},
        "yAxes": [
          {
            "label": "Rate per second",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "targets": [
          {
            "expr": "rate(bayesian_var_calculations_failed_total[5m])",
            "refId": "A",
            "legendFormat": "Calculation Failures"
          },
          {
            "expr": "rate(constitutional_prime_directive_violations_total[5m])",
            "refId": "B",
            "legendFormat": "Constitutional Violations"
          },
          {
            "expr": "rate(emergency_stops_total[5m])",
            "refId": "C",
            "legendFormat": "Emergency Stops"
          },
          {
            "expr": "rate(risk_breach_incidents_total[5m])",
            "refId": "D",
            "legendFormat": "Risk Breaches"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.01],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "30s",
          "frequency": "10s",
          "handler": 1,
          "name": "Bayesian VaR Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 5,
        "title": "E2B Sandbox Training Status",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17},
        "yAxes": [
          {
            "label": "Score / Duration",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "targets": [
          {
            "expr": "bayesian_model_accuracy_score",
            "refId": "A",
            "legendFormat": "Model Accuracy"
          },
          {
            "expr": "e2b_sandbox_utilization_percent / 100",
            "refId": "B",
            "legendFormat": "Sandbox Utilization"
          },
          {
            "expr": "rate(e2b_sandbox_training_success_total[30m])",
            "refId": "C",
            "legendFormat": "Training Success Rate"
          },
          {
            "expr": "histogram_quantile(0.95, bayesian_model_convergence_duration_seconds) / 60",
            "refId": "D",
            "legendFormat": "P95 Convergence Time (min)"
          }
        ]
      },
      {
        "id": 6,
        "title": "Portfolio Risk Exposure",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000000},
                {"color": "red", "value": 5000000}
              ]
            },
            "unit": "currencyUSD"
          }
        },
        "targets": [
          {
            "expr": "portfolio_risk_exposure_usd",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Binance Connectivity",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {"text": "DISCONNECTED", "color": "red"},
                  "1": {"text": "CONNECTED", "color": "green"}
                },
                "type": "range",
                "from": 0,
                "to": 0
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "bayesian_var_active_connections",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Pod Health Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 3},
                {"color": "green", "value": 5}
              ]
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "up{job=\"bayesian-var-production\"}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Request Volume & Response Times",
        "type": "graph",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 34},
        "yAxes": [
          {
            "label": "Requests per second",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          },
          {
            "label": "Response time (ms)",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "targets": [
          {
            "expr": "rate(bayesian_var_total_calculations[5m])",
            "refId": "A",
            "legendFormat": "VaR Calculations/sec"
          },
          {
            "expr": "histogram_quantile(0.95, bayesian_var_calculation_duration_seconds) * 1000",
            "refId": "B",
            "legendFormat": "P95 Response Time (ms)",
            "yAxis": 2
          },
          {
            "expr": "histogram_quantile(0.99, bayesian_var_calculation_duration_seconds) * 1000",
            "refId": "C",
            "legendFormat": "P99 Response Time (ms)",
            "yAxis": 2
          }
        ]
      },
      {
        "id": 10,
        "title": "Constitutional Prime Directive Events",
        "type": "logs",
        "gridPos": {"h": 10, "w": 24, "x": 0, "y": 43},
        "targets": [
          {
            "expr": "{job=\"bayesian-var-production\"} |= \"constitutional\" or \"violation\" or \"emergency\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": false,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      }
    ],
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(bayesian_var_deployments_total[5m]) > 0",
          "iconColor": "green",
          "name": "Deployments",
          "titleFormat": "Deployment Event",
          "textFormat": "New deployment detected"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {
            "text": "production",
            "value": "production"
          },
          "datasource": "prometheus",
          "definition": "label_values(up{job=~\".*bayesian.*\"}, environment)",
          "hide": 0,
          "includeAll": false,
          "label": null,
          "multi": false,
          "name": "environment",
          "options": [],
          "query": "label_values(up{job=~\".*bayesian.*\"}, environment)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        }
      ]
    }
  }
}