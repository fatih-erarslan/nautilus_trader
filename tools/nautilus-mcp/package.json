{
  "name": "@tengri/nautilus-mcp",
  "version": "1.0.0",
  "description": "High-Performance Trading Analytics MCP Server - Nautilus Trader Integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "nautilus-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:native": "cd native && cargo build --release && cp target/release/libnautilus_native.dylib ../dist/",
    "build:all": "npm run build:native && npm run build",
    "start": "bun run dist/index.js",
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "clean": "rm -rf dist native/target"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0",
    "@napi-rs/cli": "^3.5.0",
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "keywords": [
    "mcp",
    "nautilus",
    "trading",
    "analytics",
    "rust",
    "bun",
    "hyperphysics"
  ],
  "author": "Tengri Technologies <dev@tengritek.com>",
  "license": "MIT"
}
